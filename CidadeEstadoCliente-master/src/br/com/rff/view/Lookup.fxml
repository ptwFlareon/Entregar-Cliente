<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="aoLookup" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #eff;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="br.com.rff.controller.LookupController">
  <children>
    <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <bottom>
        <HBox prefHeight="62.0" prefWidth="600.0" spacing="10.0">
          <children>
            <Button fx:id="btSelecionar" mnemonicParsing="false" onAction="#selecionar" prefHeight="127.0" prefWidth="177.0" text="Selecionar" />
            <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#cancelar" prefHeight="133.0" prefWidth="153.0" text="Cancelar" />
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </HBox>
      </bottom>
      <center>
        <BorderPane id="pnPesquisa" prefHeight="600.0" prefWidth="800.0">
          <center>
            <TableView fx:id="tblDados" prefHeight="200.0" prefWidth="200.0" />
          </center>
          <top>
            <HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="200.0" spacing="10.0">
              <children>
                <TextField id="labelpesquisar" fx:id="txtPesquisa" prefWidth="344.0" promptText="Pesquisar o que procura" />
                <Button id="btnpesquisar" fx:id="btPesquisar" mnemonicParsing="false" onAction="#pesquisarClicked" text="Pesquisar" />
              </children>
              <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </padding>
            </HBox>
          </top>
        </BorderPane>
      </center>
    </BorderPane>
  </children>
</AnchorPane>
